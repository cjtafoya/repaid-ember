<form {{action 'signup' model on='submit'}}>
  <label for="username">Username</label>
  {{input id='username' value=model.username}}
  <label for="email">Email</label>
  {{input id='email' value=model.email}}
  <label for="password">Password</label>
  {{input type='password' id='password' value=model.password}}
  <label for="password">Password Cofirmation</label>
  {{input type='password' id='passwordConfirmation' value=model.passwordConfirmation}}
  <button type="submit">Sign Up!</button>
 </form>
